{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "./backend/Dockerfile"
  },
  "deploy": {
    "startCommand": "gunicorn core.wsgi --bind 0.0.0.0:$PORT",
    "healthcheckPath": "/api/health",
    "healthcheckTimeout": 120
  }
}